[ignore]
#.*/node_modules/.*
.*/__tests__/
.*/components-for-testing/.*
.*\config-overrides\.js
.*/scripts/.*

[include]
.*/node_modules/css-module-flow

[libs]
./type-def-libs

[lints]

[options]
#module.file_ext=.js
#module.file_ext=.json
#module.file_ext=.jsx
#module.file_ext=.css
#module.file_ext=.scss
#module.name_mapper.extension='\(bmp\|gif\|jpg\|jpeg\|png\|psd\|svg\|webp\|m4v\|mov\|mp4\|mpeg\|mpg\|webm\|aac\|aiff\|caf\|m4a\|mp3\|wav\|html\|pdf\)' -> 'empty/object'
module.name_mapper='^.*\.scss$' -> 'css-module-flow'
#all=true

[strict]
sketchy-null
sketchy-number
sketchy-null-string
sketchy-null-mixed
#untyped-type-import
#untyped-import
unclear-type
unsafe-getters-setters
#nonstrict-import
unnecessary-optional-chain
unnecessary-invariant
deprecated-utility
#dynamic-export