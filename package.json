    "build-demo-fcsp": "REACT_APP_CURRENT_GIT_SHA=`git rev-parse --short HEAD` ./node_modules/.bin/env-cmd -f .env.demo.fcsp react-app-rewired build",
    "build-prod-fcsp": "REACT_APP_CURRENT_GIT_SHA=`git rev-parse --short HEAD` ./node_modules/.bin/env-cmd -f .env.prod.fcsp react-app-rewired build",
    "check-libs": "./scripts/check-lib-files.sh"
