  "private": true,
  "scripts": {
    "extract-messages": "NODE_ENV=production babel ./src  --out-file /dev/null",
    "test": "./node_modules/.bin/env-cmd .env.dev react-app-rewired test --env=jsdom --verbose=false --no-cache",
    "react-scripts-test": "react-scripts test --env=jsdom",
    "analyze": "source-map-explorer build/static/js/main.*",
    "clear": "/usr/bin/osascript -e 'tell application \"System Events\" to tell process \"Terminal\" to keystroke \"k\" using command down'\n",
    "start": "./scripts/get-configs.sh && ./node_modules/.bin/env-cmd -f .env.dev.fcsp react-app-rewired start",
    "build": "REACT_APP_CURRENT_GIT_SHA=`git rev-parse --short HEAD` ./node_modules/.bin/env-cmd -f .env.dev.fcsp react-app-rewired build",
    "build-dev-towers": "REACT_APP_CURRENT_GIT_SHA=`git rev-parse --short HEAD` ./node_modules/.bin/env-cmd -f .env.dev.towers react-app-rewired build",
